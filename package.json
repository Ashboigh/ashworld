{
  "name": "enterprise-chatbot-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:push": "pnpm --filter database db:push",
    "db:generate": "pnpm --filter database db:generate",
    "db:studio": "pnpm --filter database db:studio",
    "clean": "turbo clean && rimraf node_modules",
    "generate:amalena-template": "node scripts/generate-amalena-template.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "turbo": "^2.7.6",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@10.25.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
